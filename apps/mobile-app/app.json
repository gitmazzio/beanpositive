{
  "expo": {
    "name": "Bean Positive",
    "slug": "bean-positive",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive.png",
    "scheme": "mobileapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.beanpositive.app",
      "googleServicesFile": "./services/GoogleService-Info.plist",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      },
      "entitlements": {
        "aps-environment": "development",
        "com.apple.security.application-groups": [
          "group.${ios.bundleIdentifier}.onesignal"
        ]
      }
    },
    "android": {
      "googleServicesFile": "./services/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive.png",
        "backgroundColor": "#7D8557"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.beanpositive.app"
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#7D8557",
          "image": "./assets/images/logo-white.png",
          "imageWidth": 66,
          "resizeMode": "contain"
        }
      ],
      ["expo-router"],
      ["@react-native-google-signin/google-signin"],
      "expo-notifications",
      "onesignal-expo-plugin"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "982295ab-9358-400a-b50f-9dfbdb8e2ae8"
      }
    },
    "owner": "teomazzio"
  }
}
