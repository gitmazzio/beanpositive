{
  "cli": {
    "version": ">= 0.47.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "releaseChannel": "default",
      "android": {
        "buildType": "app-bundle"
      }
    }
  }
}
